{
  "name": "datavis101",
  "version": "2.2.0",
  "description": "Data Visualisation",
  "main": "server.js",
  "scripts": {
    "start:prod": "nodemon server.js",
    "build:dev": "webpack --watch --mode development",
    "build:prod": "webpack --mode production",
    "deps-size": "webpack -p --json | webpack-bundle-size-analyzer",
    "test": "mocha --recursive",
    "test:lint:all": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "test:lint": "npm run test:lint:all -- --quiet",
    "test:lint:fix": "npm run test:lint:all -- --fix",
    "start": "npm run docker:up",
    "stop": "npm run docker:down",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neozenith/data-vis-101.git"
  },
  "keywords": [
    "data",
    "visualisation",
    "d3",
    "js"
  ],
  "author": "Josh Peak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neozenith/data-vis-101/issues"
  },
  "homepage": "https://github.com/neozenith/data-vis-101#readme",
  "eslintConfig": {
    "extends": "@neozenith"
  },
  "devDependencies": {
    "@neozenith/eslint-config": "^3.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "http-server": "^0.11.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.4",
    "style-loader": "^0.19.1",
    "supertest": "^3.1.0",
    "webpack": "^4.11.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.1.3"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "d3": "^5.4.0",
    "express": "^4.16.3",
    "mongodb": "^3.0.10",
    "mongoose": "^5.1.5",
    "morgan": "^1.9.0",
    "throng": "^4.0.0",
    "validator": "^10.3.0"
  }
}
